# Configurações do SPC
SPC_URL=https://sistema.spcbrasil.com.br
SPC_OPERADOR=108754760
SPC_SENHA=sua_senha
SPC_PALAVRA_SECRETA=sua_palavra_secreta

# Configurações de download
DOWNLOAD_PATH=./downloads

# Fonte de CNPJs - escolha UMA das opções:

# Opção 1: CNPJ único (processa uma vez e para)
CNPJ_TO_QUERY=01838723000127

# Opção 2: Arquivo Excel (fica aguardando mudanças no arquivo)
# EXCEL_FILE=./cnpjs.xlsx
# EXCEL_SHEET=CNPJs
# EXCEL_CNPJ_COLUMN=A

# Configurações do navegador
HEADLESS=false
BROWSER_TIMEOUT=30000
DEBUG=false

# Configurações de cache
CNPJ_CACHE_EXPIRATION_HOURS=24

# Configurações da TESS AI
TESS_API_KEY=sua_chave_api_tess
TESS_BASE_URL=https://tess.pareto.io
TESS_AGENT_ID=seu_agent_id
TESS_MODEL=tess-5
TESS_TEMPERATURE=1
TESS_OUTPUT_PATH=./tess_responses
# TESS_PROMPT é opcional - o agente já está configurado para processar anexos automaticamente

# =====================================================
# CONFIGURAÇÕES DO BANCO DE DADOS (OPCIONAL)
# =====================================================
# Configure estas variáveis para habilitar o salvamento no banco
DB_SERVER=localhost
DB_DATABASE=TessDataConsolidation
DB_USER=sa
DB_PASSWORD=sua-senha-aqui
DB_PORT=1433
DB_ENCRYPT=false
DB_TRUST_CERT=true

# =====================================================
# CONFIGURAÇÕES DO ATAK (Sistema de Cadastro)
# =====================================================
# URL base do sistema Atak
ATAK_BASE_URL=https://seu-sistema-atak.com.br
# Credenciais de autenticação
ATAK_USERNAME=seu_usuario
ATAK_PASSWORD=sua_senha
# Token será gerado automaticamente após autenticação
# ATAK_TOKEN=

# Configurações de cadastro
ATAK_TIPO_CADASTRO=G
ATAK_CODIGO_FILIAL=100
ATAK_CODIGO_RAMO_ATIVIDADE=037
ATAK_CODIGO_PERCURSO_ROTA=

# Valores de fallback (usados apenas se não estiverem no formulário de solicitação)
# IMPORTANTE: Os dados de carteira, lista de preço e forma de cobrança
# vêm do formulário de solicitação (tabela client_registrations)
ATAK_CODIGO_LISTA_PRECO=1
ATAK_CODIGO_CARTEIRA=101
ATAK_CODIGO_FORMA_COBRANCA=1